{
	"manifest_version": 3,
	"name": "Timer Extension",
	"version": "1.0.0",
	"description": "Hello Chroome world!",
	"icons": {
		"16": "icon.png",
		"48": "icon.png",
		"128": "icon.png"
	},
	"action": {
		"default_icon": {
			"16": "icon.png",
			"24": "icon.png",
			"32": "icon.png"
		},
		"default_title": "Timer Extension Action Title",
		"default_popup": "popup.html"
	},
	"options_page": "options.html",
	"permissions": ["storage"],
	"background": {
		"service_worker": "background.js"
	}
}
